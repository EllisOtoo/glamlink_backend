generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Vendor {
  id          String   @id @default(cuid())
  businessName String
  handle      String   @unique
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
